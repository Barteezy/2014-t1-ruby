<h1>Chemical elements</h1>

<!-- TODO: fill in this part -->
<table border="1" class=table>
  <tr>
    <th>number</th>
    <th>symbol</th>
    <th>name</th>
  </tr>
  <% Element.order(:id).each do |element| %>
    <tr>
      <td><%= element.number %></td>
      <td><%= element.symbol %></td>
      <td><%= element.name %></td>
    </tr>
  <% end %>
</table>