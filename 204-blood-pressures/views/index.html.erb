<h1> Blood Pressure Readings </h1>
<table border="1" class="table">
  <tr> 
    <th>id</th>
    <th>name</th>
    <th>systolic</th>
    <th>diastolic</th>
  </tr>
  <% Reading.order(:id).each do |r| %>
  <tr>
    <td><%=r.id%></td>
    <td><%=r.name%></td>
    <td><%=r.systolic%></td>
    <td><%=r.diastolic%></td>
  </tr>
  <%end%>
</table>
  
